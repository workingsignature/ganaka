{
  "name": "@workingsignature/ganaka",
  "version": "0.0.1",
  "description": "Ganaka SDK - TypeScript client library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "rm -rf dist",
    "test": "node test/simple-test.js",
    "test:types": "tsc --noEmit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "ganaka",
    "sdk",
    "typescript",
    "trading",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/node": "^20.10.0",
    "semantic-release": "^22.0.12",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workingsignature/ganaka.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/workingsignature/ganaka/issues"
  },
  "homepage": "https://github.com/workingsignature/ganaka#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
