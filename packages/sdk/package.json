{
  "name": "@ganaka/sdk",
  "version": "1.0.1",
  "description": "Ganaka SDK - TypeScript client library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "ganaka",
    "sdk",
    "typescript",
    "trading",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/node": "^20.10.0",
    "semantic-release": "^22.0.12",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workingsignature/ganaka.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/workingsignature/ganaka/issues"
  },
  "homepage": "https://github.com/workingsignature/ganaka#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ganaka/server-schemas": "workspace:*",
    "axios": "^1.12.2",
    "croner": "^8.0.0",
    "dayjs": "^1.11.10",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "dotenv": "^16.4.5"
  },
  "peerDependenciesMeta": {
    "dotenv": {
      "optional": true
    }
  }
}
