  },
  "keywords": [
    "ganaka",
    "types",
    "api",
    "schemas"
  ],
  "license": "MIT",
  "peerDependencies": {
    "semver": "^7.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/semver": "^7.5.6",
    "semver": "^7.5.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "zod": "^4.1.11"
  }
}
